<h1>Visit History</h1>

<table class="table">
  <thead>
    <tr>
      <td>IP Address</td>
      <td>Time</td>
    </tr>
  </thead>
  <tbody>
    <% @visit_histories.each do |visit_history| %>
      <tr>
        <td><%= visit_history.ip_address %></td>
        <td><%= visit_history.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Back to Main Page', root_path, class: 'btn btn-primary btn-xs' %>